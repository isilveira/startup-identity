<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - StartupIdentity</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                @Html.ActionLink("Account", "Index") | @Html.ActionLink("SignUp", "SignUp") | @Html.ActionLink("SignOut", "SignOut")
            </div>
        </div>

        @RenderBody()
    </div>

    @RenderSection("Scripts", required: false)
</body>
</html>